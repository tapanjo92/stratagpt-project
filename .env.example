# AWS Configuration
AWS_REGION=ap-southeast-2
AWS_ACCOUNT_ID=your-account-id

# Stack Configuration
STACK_PREFIX=StrataGPT
ENVIRONMENT=dev

# OpenSearch Configuration
OPENSEARCH_DOMAIN_NAME=strata-vectors

# S3 Configuration
DOCUMENT_BUCKET_PREFIX=strata-documents

# Bedrock Configuration
BEDROCK_MODEL_ID=amazon.titan-embed-text-v2:0
BEDROCK_EMBEDDING_DIMENSIONS=768

# Lambda Configuration
LAMBDA_MEMORY_SIZE=1024
LAMBDA_TIMEOUT_SECONDS=300

# DynamoDB Configuration
TENANT_TABLE_NAME=strata-tenants
DOCUMENT_METADATA_TABLE_NAME=strata-document-metadata

# Monitoring
CLOUDWATCH_LOG_RETENTION_DAYS=365
XRAY_SAMPLING_RATE=0.1